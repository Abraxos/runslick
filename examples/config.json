{
    "service": {
        "log_level": "DEBUG",
        "hotkey": "Alt+Q",
        "terminal": "mate-terminal --tab -e",
        "open": "xdg-open"
    },
    "magic_words": {
        "test": {
            "incantation": "echo 'Hello, world!'",
            "terminal": true,
            "hotkey": "Alt+W"
        },
        "google": {
            "incantation": {
                "format": "https://www.google.com/search?&q={search_query}",
                "search_query": "url"
            },
            "terminal": false,
            "hotkey": "Alt+G"
        },
        "syu": {
            "incantation": "sudo apt-get update --fix-missing; sudo apt-get upgrade -y; sudo apt-get dist-upgrade -y; sudo apt-get autoremove -y;",
            "terminal": true
        },
        "home": {
            "incantation": "/home/eugene"
        }
    }
}
